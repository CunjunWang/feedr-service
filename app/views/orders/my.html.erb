<title>My Orders</title>
<h1>My Orders</h1>
<% @orders.each do |order| %>
  <div>
    <p><%= "Truck: #{order.truck_id}" %></p>
    <p><%= "Order no: #{order.order_no}" %></p>
    <p><%= "Order status: #{order.order_status}" %></p>
    <p><%= "Order Subtotal: #{order.order_subtotal}" %></p>
    <p><%= "Created at: #{order.created_at}" %></p>
    <br>
  </div>
<% end %>
